<html>
    <head>
        <meta charset="utf-8">
    </head>
    <body>
        <h1> List books </h1>

        <table id="books">
            <tr>
                <td>Title</td>
                <td>Price</td>
                <td>Edit</td>
                <td>Delete</td>
            </tr>
            <tr id="book_${book.id}" for (book in data.books)>

                <td>${book.titulo}</td>
                <td>${book.preco}</td>
                <td><a href="/book/find/${book.id}">Edit</a></td>
                <td><a href="#" data-ref="${book.id}" data-type="delete">Delete</a></td> 
            </tr>
        </table> 

        <script src="/static/js/remove-book.js">
        </script>

    </body> 
</html>